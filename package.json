    "pre-commit:typecheck": "tsc --noEmit",
    "translate": "run-p 'translate:*'",
    "translate:md": "node ./scripts/translateMarkdown.mjs",
    "translate:json": "node ./scripts/translateJson.mjs",
    "send:sitemap": "node ./scripts/sendSitemap.mjs"
